        Scanner scn = new Scanner(System.in);
        int getInteger = scn.nextInt();
        double getFloat = scn.nextDouble();
        scn.nextLine();
        String getString = scn.nextLine();
        
        
        
        System.out.println("String: "+getString);
        System.out.println("Double: "+getFloat);
        System.out.println("Int: "+getInteger);
    }
